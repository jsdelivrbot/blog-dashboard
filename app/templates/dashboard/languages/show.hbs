<div class="container">
  {{#each model.frameworks as |framework|}}
    {{#link-to "dashboard.frameworks.show" framework.id class="card"}}
      {{framework.name}}
    {{/link-to}}
  {{/each}}
  {{#if admin}}
    {{#link-to "dashboard.frameworks.new" model.id class="primary-button btn large"}}
      <span>+</span>
    {{/link-to}}
  {{/if}}
  {{#each posts as |post|}}
  {{#link-to "dashboard.posts.show"  post.id class="block"}}
    {{post.title}}
  {{/link-to}}
  {{/each}}
  {{#link-to "dashboard.posts.new"  (query-params  framework_id=null language_id=model.id) class="primary-button btn long"}} 
  <span>+</span>
  {{/link-to}}
  <div class="pagination-buttons">
    {{#link-to "dashboard.languages.show"  model.slug (query-params page=navigate.previous_page) class=(if navigate.first 'inactive') id="left-arrow"}}
    {{/link-to}}

    {{#link-to "dashboard.languages.show"  model.slug (query-params page=navigate.next_page)  id="right-arrow" class=(if navigate.last 'inactive')}}
    {{/link-to}}
  </div>
</div>