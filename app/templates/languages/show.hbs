<div class="container">
  {{#each model.frameworks as |framework|}}
    {{#link-to "frameworks.show" framework.id class="card"}}
      {{framework.name}}
    {{/link-to}}
  {{/each}}
  {{#each model.posts as |post|}}
  {{#link-to "posts.show"  post.id class="block"}}
    {{post.title}}
  {{/link-to}}
  {{/each}}
</div>